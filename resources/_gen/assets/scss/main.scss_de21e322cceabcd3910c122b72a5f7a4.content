.light-theme .mega-menu-container {
  font-family: "Calibri", sans-serif; }

.light-theme .mega-menu-container,
.light-theme .mega-menu-container * {
  box-sizing: border-box;
  z-index: 2; }

.light-theme .mega-menu-container a {
  text-decoration: none;
  color: white;
  vertical-align: top; }

.light-theme .mega-menu-container .menu-title {
  font-size: 18px; }

.light-theme .mega-menu-container ul {
  padding: 0;
  margin: 0;
  list-style: none; }

.light-theme .mega-menu-container h4 {
  margin: 10px 0;
  height: 2rem; }

.light-theme .mega-menu {
  position: absolute;
  margin: 0 auto;
  margin-left: calc( 350px + 20px + 5px);
  margin-top: calc( 100px * 0.6);
  max-width: 100%; }

.light-theme .mega-menu > ul {
  display: flex;
  margin: 0 auto;
  justify-content: center;
  align-items: center;
  gap: 10px; }

.light-theme .mega-menu-container .menu-title {
  padding: 10px 16px;
  display: flex;
  justify-content: center; }

.light-theme .mega-menu .menu-title {
  position: relative; }

.light-theme .mega-menu .menu-title::after {
  content: "";
  height: 0;
  width: 0;
  background: #fcd116;
  position: absolute;
  left: 0;
  bottom: 0;
  transition: all 150ms; }

.light-theme .mega-menu ul li:hover > .menu-title::after {
  height: 4px;
  width: 100%; }

.light-theme .mega-menu-container .submenu-container-colLeft {
  opacity: 0;
  position: absolute;
  pointer-events: none;
  top: 34px;
  transform: translateY(20px);
  transition: all 200ms ease; }

.light-theme .mega-menu-container .submenu-container-colRight {
  opacity: 0;
  position: absolute;
  pointer-events: none;
  right: 0;
  top: 34px;
  transform: translateY(20px);
  transition: all 200ms ease; }

.light-theme .mega-menu-container .submenu-container {
  opacity: 0;
  position: absolute;
  pointer-events: none;
  left: 0;
  top: 34px;
  transform: translateY(20px);
  transition: all 150ms ease; }

.light-theme .submenu .colums img {
  vertical-align: bottom;
  margin-top: 0px;
  height: 60px;
  width: 65px; }

.light-theme .mega-menu .has-submenu:hover .submenu-container-colLeft {
  opacity: 1;
  pointer-events: auto;
  transform: translateY(0); }

.light-theme .mega-menu .has-submenu:hover .submenu-container-colRight {
  opacity: 1;
  pointer-events: auto;
  transform: translateY(0); }

.light-theme .mega-menu .has-submenu:hover .submenu-container {
  opacity: 1;
  pointer-events: auto;
  transform: translateY(0); }

.light-theme .submenu .colums {
  display: flex;
  font-size: 0.9em;
  gap: 32px;
  border-top: 1px solid black; }

.light-theme .submenu .colums .submenu-column {
  flex: 1; }

.light-theme .submenu {
  background-color: rgba(255, 255, 255, 0.956);
  padding: 16px 24px;
