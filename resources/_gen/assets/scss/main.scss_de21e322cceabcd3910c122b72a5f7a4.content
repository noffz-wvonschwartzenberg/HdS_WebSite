/* Common */
.light-theme .home-main {
  margin: 0;
  padding: 0;
  background-color: #e3e2d4; }

.light-theme .header {
  background-color: #e3e2d4; }
  .light-theme .header .header-container {
    margin: 0 auto;
    width: 100%;
    height: 100px;
    background-color: #007a5e; }
  .light-theme .header .header-logo {
    z-index: 2;
    position: absolute;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 350px;
    height: 120px;
    margin-left: 20px;
    background-color: white;
    border: 1px solid #007a5e;
    transition: transform 0.15s ease, box-shadow 0.15s ease; }
  .light-theme .header .logo {
    width: 90%;
    height: auto;
    display: block; }
  .light-theme .header .header-logo:hover {
    transform: scale(1.05);
    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2); }
  .light-theme .header .logo-klein {
    height: auto;
    width: 90%;
    display: none; }
  .light-theme .header .header-donate-paypal {
    z-index: 2;
    position: absolute;
    margin-left: 10px;
    margin-top: 130px; }
  .light-theme .header .header-menu {
    display: flex;
    justify-content: center;
    z-index: 1;
    position: absolute; }
  .light-theme .header .gray {
    font-size: calc( 100px * 0.6 * 0.25);
    width: 100%;
    margin-top: 0px;
    height: calc( 100px * 0.6);
    justify-content: right;
    background-color: #e3e2d4; }
  .light-theme .header .header-gray-item {
    display: flex;
    margin: 0 3px;
    padding: 0 0;
    place-items: center;
    font-weight: lighter;
    color: black; }
    .light-theme .header .header-gray-item:hover {
      text-decoration: underline; }
    .light-theme .header .header-gray-item::before {
      content: "";
      position: relative;
      left: -3px;
      height: calc( 100px * 0.6 * 0.25);
      width: 2px;
      background-color: #007a5e; }
    .light-theme .header .header-gray-item:first-child::before {
      display: none; }
    .light-theme .header .header-gray-item:last-child {
      padding-right: 30px; }

@media (max-width: 1080px) {
  .light-theme .header .header-logo {
    width: 130px; }
  .light-theme .header .logo {
    display: none; }
  .light-theme .header .logo-klein {
    display: inline; }
  .light-theme .header .mega-menu {
    margin-left: calc( 130px + 20px + 5px);
